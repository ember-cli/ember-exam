{
  "name": "ember-exam",
  "version": "5.0.1",
  "description": "Run your tests with randomization, splitting, and parallelization for beautiful tests.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-cli/ember-exam.git"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "coverage": "nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:mocha": "TEST_FRAMEWORK=ember-mocha bin/install-test-framework.sh && ember test",
    "test:node": "nyc mocha 'node-tests/**/*-test.js'"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "debug": "^4.1.0",
    "ember-cli-babel": "^7.21.0",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.0",
    "npmlog": "^4.1.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "silent-error": "^1.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "codeclimate-test-reporter": "^0.5.0",
    "ember-cli": "^3.21.2",
    "ember-cli-addon-docs": "^0.6.15",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-htmlbars-inline-precompile": "^3.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-shims": "^1.1.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.20.3",
    "ember-source-channel-url": "^1.1.0",
    "ember-template-lint": "^2.9.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "fixturify": "^1.2.0",
    "istanbul": "^0.4.3",
    "loader.js": "^4.7.0",
    "mocha": "^8.1.0",
    "mocha-eslint": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rsvp": "^4.8.5",
    "sinon": "^9.0.3",
    "testdouble": "^3.16.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "nyc": {
    "exclude": [
      "bower_components",
      "config",
      "node-tests",
      "tests"
    ]
  },
  "homepage": "https://ember-cli.github.io/ember-exam"
}
